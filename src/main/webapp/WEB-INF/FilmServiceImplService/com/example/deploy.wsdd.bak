<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from FilmServiceImplService WSDL service -->

  <service name="FilmServiceImpl" provider="java:RPC" style="document" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://example.com"/>
      <parameter name="wsdlServiceElement" value="FilmServiceImplService"/>
      <parameter name="schemaQualified" value="http://example.com"/>
      <parameter name="wsdlServicePort" value="FilmServiceImpl"/>
      <parameter name="className" value="com.example.FilmServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="FilmServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getFilm" qname="getFilm" returnQName="retNS:getFilmReturn" xmlns:retNS="http://example.com" returnType="rtns:FilmEntity" xmlns:rtns="http://example.com" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://example.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="deleteFilm" qname="deleteFilm" returnQName="retNS:deleteFilmReturn" xmlns:retNS="http://example.com" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id1" xmlns:pns="http://example.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getAllFilms" qname="getAllFilms" returnQName="retNS:getAllFilmsReturn" xmlns:retNS="http://example.com" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://example.com" returnItemQName="tns:item" xmlns:tns="http://example.com" soapAction="" >
      </operation>
      <operation name="createFilm" qname="createFilm" returnQName="retNS:createFilmReturn" xmlns:retNS="http://example.com" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:filmEntity" xmlns:pns="http://example.com" type="tns:FilmEntity" xmlns:tns="http://example.com"/>
      </operation>
      <operation name="updateFilm" qname="updateFilm" returnQName="retNS:updateFilmReturn" xmlns:retNS="http://example.com" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id2" xmlns:pns="http://example.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getAllFilms getFilm createFilm deleteFilm updateFilm"/>

      <typeMapping
        xmlns:ns="http://example.com"
        qname="ns:FilmEntity"
        type="java:com.example.FilmEntity"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://example.com"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
  </service>
</deployment>
